{
  "name": "think-feel-be-therapy",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "dotenvx run --convention=nextjs -- next build",
    "dev": "dotenvx run --convention=nextjs -- next dev --turbo",
    "format": "biome format",
    "format:fix": "biome format --write",
    "generate:types": "payload generate:types",
    "lint": "biome lint",
    "lint:fix": "biome lint --write",
    "start": "dotenvx run --convention=nextjs -- next start",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.45.1",
    "@payloadcms/db-mongodb": "^3.43.0",
    "@payloadcms/next": "^3.43.0",
    "@payloadcms/richtext-lexical": "^3.43.0",
    "@payloadcms/ui": "^3.43.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel": "^8.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-fade": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.11.0",
    "html-react-parser": "^5.2.5",
    "next": "15.3.4",
    "payload": "^3.43.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "sharp": "^0.34.2",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.4",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "typescript": "^5.8.3",
    "ultracite": "5.0.0--canary.162.43587b3.1"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ]
  },
  "watch": {
    "generate:types": {
      "patterns": [
        "src"
      ],
      "extensions": "ts,tsx",
      "quiet": false
    }
  }
}
